<% if current_user.liked? @post %>
  $('.like-btn').addClass('liked');
  $('.btn-clr').css('color', 'red');
<% else %>
  $('.like-btn').removeClass('liked');
  $('.btn-clr').css('color', 'black');
<% end %> 
$("#post-<%= @post.id %>").addClass("liked").html('<%= @post.get_upvotes.size %>');/*$(documnet).ready(function(){
